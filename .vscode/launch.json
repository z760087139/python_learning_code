        {
            "name": "Conda Scrapy 3.7",
            "pythonPath": "D:\\python\\Anaconda\\envs\\anaconda_3.7\\python.exe",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "stopOnEntry": true
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "stopOnEntry": true
        },
        {
            "name": "Python 2.7",
            "pythonPath": "F:\\learning\\vir_python2.7\\python2.7\\Scripts\\python.exe",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "stopOnEntry": true
        },
        {
            "name": "Python 3.7",
            "pythonPath": "F:\\learning\\vir_python3.7\\python3.7\\Scripts\\python.exe",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "stopOnEntry": true
        },
        {
            "name": "Hzl",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "stopOnEntry": true,
            "pythonPath": "F:\\learning\\hzl\\Scripts\\python.exe",
        },
        {
            "name": "Chrome current file",
            "type": "chrome",
            "request": "launch",
            "file": "${file}"
        },
        {
            "name": "Python: Attach",
            "type": "python",
            "request": "attach",
            "localRoot": "${workspaceFolder}",
            "remoteRoot": "${workspaceFolder}",
            "port": 3000,
            "secret": "my_secret",
            "host": "localhost"
        },
        {
            "name": "Python: Terminal (integrated)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Terminal (external)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "externalTerminal"
        },
        {
            "name": "Python: Django",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/web_code/manage.py",
            "args": [
                "runserver",
                "--noreload",
                "--nothreading"
            ],
            "debugOptions": [
                "RedirectOutput",
                "Django"
            ]
        },
        {
            "name": "Python: Flask (0.11.x or later)",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "app.py"
            },
            "args": [
                "run",
                "--no-debugger",
                "--no-reload"
            ]
        },
        {
            "name": "Python: Module",
            "type": "python",
            "request": "launch",
            "module": "module.name"
        },
        {
            "name": "Python: Pyramid",
            "type": "python",
            "request": "launch",
            "args": [
                "${workspaceFolder}/development.ini"
            ],
            "debugOptions": [
                "RedirectOutput",
                "Pyramid"
            ]
        },
        {
            "name": "Python: Watson",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/console.py",
            "args": [
                "dev",
                "runserver",
                "--noreload=True"
            ]
        },
        {
            "name": "Python: All debug Options",
            "type": "python",
            "request": "launch",
            "pythonPath": "${config:python.pythonPath}",
            "program": "${file}",
            "module": "module.name",
            "env": {
                "VAR1": "1",
                "VAR2": "2"
            },
            "envFile": "${workspaceFolder}/.env",
            "args": [
                "arg1",
                "arg2"
            ],
            "debugOptions": [
                "RedirectOutput"
            ]
        }
    ]
}